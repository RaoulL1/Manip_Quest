	number = {8},
	urldate = {2020-09-28},
	journal = {PLoS ONE},
	author = {Philippi, Carissa L. and Feinstein, Justin S. and Khalsa, Sahib S. and Damasio, Antonio and Tranel, Daniel and Landini, Gregory and Williford, Kenneth and Rudrauf, David},
