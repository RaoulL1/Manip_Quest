	file = {Tajadura-Jiménez et al. - 2012 - The person in the mirror Using the enfacement ill.pdf:C\:\\Users\\lisa\\Zotero\\storage\\L67MJMSC\\Tajadura-Jiménez et al. - 2012 - The person in the mirror Using the enfacement ill.pdf:application/pdf},
