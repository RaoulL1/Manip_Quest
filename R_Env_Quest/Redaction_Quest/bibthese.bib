	author = {Gauthier, Baptiste and Bréchet, Lucie and Lance, Florian and Mange, Robin and Herbelin, Bruno and Faivre, Nathan and Bolton, Thomas A.W. and Ville, Dimitri Van De and Blanke, Olaf},
