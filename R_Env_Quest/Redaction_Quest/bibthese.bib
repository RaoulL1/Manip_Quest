	journal = {Psychological Medicine},
	author = {Berner, L. A. and Simmons, A. N. and Wierenga, C. E. and Bischoff-Grethe, A. and Paulus, M. P. and Bailer, U. F. and Ely, A. V. and Kaye, W. H.},
