@article{zamariola_can_2017,
	title = {Can you feel the body that you see? {On} the relationship between interoceptive accuracy and body image},
	volume = {20},
	issn = {17401445},
	shorttitle = {Can you feel the body that you see?},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S174014451630198X},
