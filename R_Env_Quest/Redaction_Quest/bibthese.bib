	year = {2012},
	pages = {e35912},
	file = {Petkova et al. - 2012 - Rubber Hands Feel Touch, but Not in Blind Individu.PDF:C\:\\Users\\lisa\\Zotero\\storage\\FHCR6DWX\\Petkova et al. - 2012 - Rubber Hands Feel Touch, but Not in Blind Individu.PDF:application/pdf},
