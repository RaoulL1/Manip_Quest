	journal = {Brain and Cognition},
	author = {Sakai, Katsuya and Goto, Keisuke and Watanabe, Rui and Tanabe, Junpei and Amimoto, Kazu and Kumai, Ken and Shibata, Keiichiro and Morikawa, Kenji and Ikeda, Yumi},
