@article{keizer_too_2013,
	title = {Too {Fat} to {Fit} through the {Door}: {First} {Evidence} for {Disturbed} {Body}-{Scaled} {Action} in {Anorexia} {Nervosa} during {Locomotion}},
	volume = {8},
