	number = {3},
	urldate = {2022-02-06},
	journal = {Biological Psychiatry},
	author = {Bernardoni, Fabio and Geisler, Daniel and King, Joseph A. and Javadi, Amir-Homayoun and Ritschel, Franziska and Murr, Julia and Reiter, Andrea M.F. and Rössner, Veit and Smolka, Michael N. and Kiebel, Stefan and Ehrlich, Stefan},
