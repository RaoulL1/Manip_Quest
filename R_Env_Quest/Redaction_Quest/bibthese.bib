	pages = {3304--3316},
	file = {Ehrsson et al. - 2003 - Imagery of Voluntary Movement of Fingers, Toes, an.pdf:C\:\\Users\\lisa\\Zotero\\storage\\W4WYJXWB\\Ehrsson et al. - 2003 - Imagery of Voluntary Movement of Fingers, Toes, an.pdf:application/pdf},
}

