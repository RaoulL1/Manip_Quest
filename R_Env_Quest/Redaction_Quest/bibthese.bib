	journal = {Journal of the International Neuropsychological Society},
	author = {Purcell, Juliann B. and Winter, Samantha R. and Breslin, Caitlin M. and White, Nicole C. and Lowe, Michael R. and Branch Coslett, H.},
	month = aug,
	year = {2018},
