@article{pitron_beyond_2017,
	title = {Beyond differences between the body schema and the body image: insights from body hallucinations},
	volume = {53},
	issn = {10538100},
	shorttitle = {Beyond differences between the body schema and the body image},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1053810017300612},
	doi = {10.1016/j.concog.2017.06.006},
	abstract = {The distinction between the body schema and the body image has become the stock in trade of much recent work in cognitive neuroscience and philosophy. Yet little is known about the interactions between these two types of body representations. We need to account not only for their dissociations in rare cases, but also for their convergence most of the time. Indeed in our everyday life the body we perceive does not conﬂict with the body we act with. Are the body image and the body schema then somehow reshaping each other or are they relatively independent and do they only happen to be congruent? On the basis of the study of bodily hallucinations, we consider which model can best account for the body schema/body image interactions.},
	language = {en},
	urldate = {2019-11-18},
	journal = {Consciousness and Cognition},
	author = {Pitron, Victor and de Vignemont, Frédérique},
