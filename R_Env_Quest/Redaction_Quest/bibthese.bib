	issn = {18789293},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1878929318301609},
	doi = {10.1016/j.dcn.2019.100626},
	abstract = {The self-concept – the set of beliefs that a person has about themselves – shows signiﬁcant development from adolescence to early adulthood, in parallel with brain development over the same period. We sought to investigate how age-related changes in self-appraisal processes corresponded with brain network segregation and integration in healthy adolescents and young adults. We scanned 88 participants (46 female), aged from 15 to 25 years, as they performed a self-appraisal task. We ﬁrst examined their patterns of activation to self-appraisal, and replicated prior reports of reduced dorsomedial prefrontal cortex activation with older age, with similar reductions in precuneus, right anterior insula/operculum, and a region extending from thalamus to striatum. We used independent component analysis to identify distinct anterior and posterior components of the default mode network (DMN), which were associated with the self-appraisal and rest-ﬁxation parts of the task, respectively. Increasing age was associated with reduced functional connectivity between the two components. Finally, analyses of task-evoked interactions between pairs of nodes within the DMN identiﬁed a subnetwork that demonstrated reduced connectivity with increasing age. Decreased network integration within the DMN appears to be an important higher-order maturational process supporting the emerging adult self.},
	language = {en},
	urldate = {2020-09-28},
	journal = {Developmental Cognitive Neuroscience},
	author = {Davey, Christopher G. and Fornito, Alex and Pujol, Jesus and Breakspear, Michael and Schmaal, Lianne and Harrison, Ben J.},
	month = apr,
	year = {2019},
	pages = {100626},
	file = {Davey et al. - 2019 - Neurodevelopmental correlates of the emerging adul.pdf:C\:\\Users\\lisa\\Zotero\\storage\\JGTR9WQF\\Davey et al. - 2019 - Neurodevelopmental correlates of the emerging adul.pdf:application/pdf},
}

@article{clark_whatever_2013,
