	pages = {F16--F25},
	file = {Peelen et al. - 2009 - Differential development of selectivity for faces .pdf:C\:\\Users\\lisa\\Zotero\\storage\\WYB989VI\\Peelen et al. - 2009 - Differential development of selectivity for faces .pdf:application/pdf},
