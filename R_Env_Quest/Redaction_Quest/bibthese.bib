	pages = {479},
	file = {Posada-Quintero et Chon - 2020 - Innovations in Electrodermal Activity Data Collect.pdf:C\:\\Users\\lisa\\Zotero\\storage\\G53EBHQH\\Posada-Quintero et Chon - 2020 - Innovations in Electrodermal Activity Data Collect.pdf:application/pdf},
}

@inproceedings{salahuddin_ultra_2007,
