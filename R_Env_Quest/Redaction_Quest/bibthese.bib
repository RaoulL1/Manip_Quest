	journal = {International Journal of Eating Disorders},
	author = {Levinson, Cheri A. and Brosof, Leigh C. and Vanzhula, Irina and Christian, Caroline and Jones, Payton and Rodebaugh, Thomas L. and Langer, Julia K. and White, Emily K. and Warren, Cortney and Weeks, Justin W. and Menatti, Andrew and Lim, Michelle H. and Fernandez, Katya C.},
	month = jul,
