@article{frank_association_2018,
	title = {Association of {Brain} {Reward} {Learning} {Response} {With} {Harm} {Avoidance}, {Weight} {Gain}, and {Hypothalamic} {Effective} {Connectivity} in {Adolescent} {Anorexia} {Nervosa}},
