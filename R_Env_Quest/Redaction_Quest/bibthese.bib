@article{tsakiris_multisensory_2017,
	title = {The multisensory basis of the self: {From} body to identity to others},
	volume = {70},
	issn = {1747-0218, 1747-0226},
	shorttitle = {The multisensory basis of the self},
	url = {http://journals.sagepub.com/doi/10.1080/17470218.2016.1181768},
	doi = {10.1080/17470218.2016.1181768},
	abstract = {By grounding the self in the body, experimental psychology has taken the body as the starting point for a science of the self. One fundamental dimension of the bodily self is the sense of body ownership that refers to the special perceptual status of one’s own body, the feeling that “my body” belongs to me. The primary aim of this review article is to highlight recent advances in the study of body ownership and our understanding of the underlying neurocognitive processes in three ways. I ﬁrst consider how the sense of body ownership has been investigated and elucidated in the context of multisensory integration. Beyond exteroception, recent studies have considered how this exteroceptively driven sense of body ownership can be linked to the other side of embodiment, that of the unobservable, yet felt, interoceptive body, suggesting that these two sides of embodiment interact to provide a unifying bodily self. Lastly, the multisensorial understanding of the self has been shown to have implications for our understanding of social relationships, especially in the context of self–other boundaries. Taken together, these three research strands motivate a uniﬁed model of the self inspired by current predictive coding models.},
	language = {en},
	number = {4},
	urldate = {2019-11-18},
	journal = {Quarterly Journal of Experimental Psychology},
	author = {Tsakiris, Manos},
	month = apr,
