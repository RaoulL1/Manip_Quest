	pages = {655--668},
	file = {Longo et al. - 2010 - More than skin deep Body representation beyond pr.pdf:C\:\\Users\\lisa\\Zotero\\storage\\EJVS39TU\\Longo et al. - 2010 - More than skin deep Body representation beyond pr.pdf:application/pdf;Longo et al. - 2010 - More than skin deep Body representation beyond pr.pdf:C\:\\Users\\lisa\\Zotero\\storage\\KJ7GREZG\\Longo et al. - 2010 - More than skin deep Body representation beyond pr.pdf:application/pdf;Longo et al. - 2010 - More than skin deep Body representation beyond pr.pdf:C\:\\Users\\lisa\\Zotero\\storage\\XBJHQFH9\\Longo et al. - 2010 - More than skin deep Body representation beyond pr.pdf:application/pdf},
