@article{allen_overconcern_2008,
	title = {Overconcern with weight and shape is not the same as body dissatisfaction: {Evidence} from a prospective study of pre-adolescent boys and girls},
	volume = {5},
	issn = {17401445},
	shorttitle = {Overconcern with weight and shape is not the same as body dissatisfaction},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1740144508000466},
	doi = {10.1016/j.bodyim.2008.03.005},
	abstract = {Overconcern with weight and shape and body dissatisfaction have both emerged as signiﬁcant predictors of disordered eating. However, it is unclear how these constructs relate to each other, and if each has different antecedents and consequences. This study aimed to identify prospective predictors of each construct and to determine their relative importance in predicting dietary restraint and binge eating. Eight- to 13-year-old boys and girls (N = 259) were assessed at baseline and one-year follow-up, using a range of measures that included the Child Eating Disorder Examination. Psychosocial variables predicted overconcern with weight and shape whilst objective weight predicted body dissatisfaction. Body dissatisfaction and weight and shape concern predicted restraint, and weight and shape concern and restraint predicted binge eating. Findings provide support for the theoretical differences between body dissatisfaction and overconcern with weight and shape, and highlight the importance of focusing on speciﬁc body image variables.},
	language = {en},
	number = {3},
	urldate = {2019-11-18},
	journal = {Body Image},
	author = {Allen, Karina L. and Byrne, Susan M. and McLean, Neil J. and Davis, Elizabeth A.},
	month = sep,
	year = {2008},
	pages = {261--270},
	file = {Allen et al. - 2008 - Overconcern with weight and shape is not the same .pdf:C\:\\Users\\lisa\\Zotero\\storage\\IHEF7AWI\\Allen et al. - 2008 - Overconcern with weight and shape is not the same .pdf:application/pdf;Allen et al. - 2008 - Overconcern with weight and shape is not the same .pdf:C\:\\Users\\lisa\\Zotero\\storage\\88DJZDYD\\Allen et al. - 2008 - Overconcern with weight and shape is not the same .pdf:application/pdf},
