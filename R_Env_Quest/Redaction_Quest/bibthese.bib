	pages = {349--365},
	file = {Yentes et al. - 2013 - The Appropriate Use of Approximate Entropy and Sam.pdf:C\:\\Users\\lisa\\Zotero\\storage\\HQKDHYYK\\Yentes et al. - 2013 - The Appropriate Use of Approximate Entropy and Sam.pdf:application/pdf},
